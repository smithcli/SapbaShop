{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:node/recommended"
    ],
    "plugins": [
      "prettier",
      "node"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn"
    }
}
