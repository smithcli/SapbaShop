{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:node/recommended"
    ],
    "plugins": [
      "prettier",
      "node"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "spaced-comment": "off",
      "no-console": "warn"
    }
}
